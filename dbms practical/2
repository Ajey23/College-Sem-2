CREATE TABLE CUSTOMER
	(ID VARCHAR (5) PRIMARY KEY , 
	NAME VARCHAR (20) UNIQUE NOT NULL, 
	AGE INT, 
	ADDRESS VARCHAR(30) DEFAULT 'Ahmedabad',
	 SALARY DECIMAL(8,2),
	 DOB DATE);


INSERT INTO CUSTOMER VALUES ("C1", "Ramesh", 32, "Ahmedabad", '2000.00', '1987-02-12');

