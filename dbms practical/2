SELECT *FROM CUSTOMER;


SELECT ID, NAME, ADDRESS FROM CUSTOMER;


SELECT NAME, ADDRESS
	FROM CUSTOMER
	WHERE ID='C7';


SELECT ID, NAME, SALARY 
	FROM CUSTOMER
	WHERE SALARY>1500;


SELECT *FROM CUSTOMER
	WHERE SALARY>5000


SELECT SALARY 
	FROM CUSTOMER 
	WHERE ID='C7';


SELECT AGE, NAME 
	FROM CUSTOMER 
	WHERE SALARY >=1000.00 


